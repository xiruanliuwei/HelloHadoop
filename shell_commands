
$ mvn archetype:generate -DgroupId=com.hellohadoop -DartifactId=HelloHadoop -DarchetypeArtifactId=maven-archetype-quickstart -DinteractiveMode=false

$ mvn eclipse:eclipse -DdownloadSources=true -DdownloadJavadocs=true

$ mvn help:help
$ mvn help:dependency
$ mvn exec:help
$ mvn eclipse:help
$ mvn plugin:help

